{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "tsconfigRootDir": ".",
            "project": [
                "./tsconfig.json"
            ]
    },
    "rules": {
        "react/jsx-filename-extension": [
            0
        ],
        "no-console": 2,
        "prettier/prettier": 2
    },
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "import"
    ],
    "extends": [
        "airbnb-typescript",
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ]
}